   ],
   "source": [
    "# input\n",
    "celsius = float(input(\"Enter Temperature in Celsius : \"))\n",
    "# 1 fahrenheit = (celsius * 9/5) + 32\n",
    "fahrenheit = (celsius * 9/5) + 32 \n",
    "print(f\"{celsius} celsius is equal to {fahrenheit} fahrenheit\")"
